<div>
    <h1 class="p-title">Asset</h1>

    <div>
        <span class="p-key">Title: </span>
        <span class="p-value">{{detail.key_title}}</span>
    </div>
    <div>
        <span class="p-key">Entity Name: </span>
        <span class="p-value">{{detail.entityname}}</span>
    </div>
    <div>
        <span class="p-key">Status: </span>
        <span class="p-value">{{detail.key_status}}</span>
    </div>
    <div>
        <span class="p-key">Consequence: </span>
        <span class="p-value">{{detail.key_consequence}}</span>
    </div>
    <div>
        <span class="p-key">OS: </span>
        <span class="p-value">{{detail.key_os}}</span>
    </div>
    <div>
        <span class="p-key">Severity: </span>
        <span class="p-value">{{detail.key_severity}}</span>
    </div>
    <div>
        <span class="p-key">CVE ID: </span>
        <a class="p-link" href="{{detail.key_cve.url1}}">{{detail.key_cve.id1}}</a>
    </div>
    <div>
        <span class="p-key">Type: </span>
        <span class="p-value">{{detail.key_type}}</span>
    </div>
    <div>
        <span class="p-key">Category: </span>
        <span class="p-value">{{detail.key_category}}</span>
    </div>
</div>